@model DTO.Models.TrainingsSwimmersSwimStyleDTO
    @{
        ViewBag.Title = "Training";
    }
<h2>Training </h2>
<h3>Swimmer @Model.FirstName @Model.LastName</h3>
    <div>
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.TrainingId</dd>

            <dt>First Name</dt>
            <dd>@Model.FirstName</dd>
            <dt>Last Name</dt>
            <dd>@Model.LastName</dd>
            <dt>Style</dt>
            <dd>@Model.Style</dd>
            <dt>Distance</dt>
            <dd>@Model.Distance</dd>
            <dt>Training Date</dt>
            <dd>@Model.TrainingDate</dd>
        </dl>
    </div>
